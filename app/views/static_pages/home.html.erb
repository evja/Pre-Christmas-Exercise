<h1 class="page-title">The Missing Link</h1>

<% if !current_user %>
  <div class="span7 text-center">
    <%= link_to 'Sign Up', new_user_registration_path, class: "btn btn-info" %>
  </div>
<% else %>
  <div class="span7 text-center">
    <%= link_to 'My Links', user_links_path(current_user), class: "btn btn-warning" %>
  </div>
<% end  %>